{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": {
            "severity": "warning",
            "options": [
                "log",
                "error",
                "debug",
                "info",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "object-literal-sort-keys": false,
        "semicolon": [
            true,
            "never"
        ],
        "indent": [
            true,
            "spaces",
            2
        ],
        "quotemark": [
            true,
            "single"
        ]
    },
    "rulesDirectory": []
}